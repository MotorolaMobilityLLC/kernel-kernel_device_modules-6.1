/dts-v1/;
/plugin/;

#include "mediatek/mt6878-aito-common-overlay.dtsi"

#define AITO_NA_RADIO 1

/* Board match root node */
/ {
	fragment@model {
		target-path = "/";
		__overlay__ {
			mot,model = "aito";
			mot,board-id = <(AITO_NA_RADIO << 8 | 0x42) 0xB200>;
		};
	};
};

&mt6375_typec {
       mmi,support_CID;
};

&i2c0 {
	wt6670f@35 {
		status = "disabled";
	};
};

&charger {
	/*pe5 && pdc && wlc*/
	fast_charging_indicator = <0x8014>;
	mmi,wls-boost-using-otg;
};

&mmi_pehv {
	status = "disabled";
};

&cps_wls_charger {
	wireless-tx-support;
	config-otg-support;
	config-otg-vout = <5500000>;
	config-otg-iout = <1800000>;
};

#include <aito_dvt2/cust.dtsi>
